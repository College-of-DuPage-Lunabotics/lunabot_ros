{
  "name": "Lunabot Workspace",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "..",
    "args": {
      "USERNAME": "rosdev",
      "UID": "1000",
      "GID": "1000",
      "GPU_SUPPORT": "false",
      "GPU_TYPE": "none"
    }
  },
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/lunabot_ws/src,type=bind",
  "workspaceFolder": "/home/lunabot_ws",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-vscode.cpptools",
        "ms-iot.vscode-ros"
      ]
    }
  },
  "remoteUser": "rosdev"
}
