<?xml version="1.0"?>
<robot name="trencher_bot"
  xmlns:xacro="http://www.ros.org/wiki/xacro">

  <material name="light_grey">
    <color rgba="0.61 0.61 0.61 1.0" />
  </material>

  <material name="camera_grey">
    <color rgba="0.81 0.81 0.81 1.0" />
  </material>

  <material name="black">
    <color rgba="0.22 0.22 0.22 1.0" />
  </material>

  <material name="white">
    <color rgba="0.89 0.89 0.89 1.0" />
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/trencher_bot/trencher_bot.dae" />
      </geometry>
      <origin xyz="0 0 0.1625" rpy="0 0 0" />
      <material name="light_grey" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/trencher_bot/trencher_bot.stl" />
      </geometry>
      <origin xyz="0 0 0.085" rpy="0 0 0" />
    </collision>
    <inertial>
      <mass value="25.0" />
      <origin xyz="0 0 0.085" />
      <inertia ixx="1.3669" ixy="0.0" ixz="0.0" iyy="3.3602" iyz="0.0" izz="4.7271" />
    </inertial>
  </link>

  <link name="lidar_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/s3_lidar/s3_lidar.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 -1.5708" />
      <material name="black" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/s3_lidar/s3_lidar.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 -1.5708" />
    </collision>
  </link>

  <link name="d455_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/d455_camera/d455_camera.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 3.14" />
      <material name="camera_grey" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/d455_camera/d455_camera.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 3.14" />
    </collision>
  </link>

  <link name="d456_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/d456_camera/d456_camera.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <material name="camera_grey" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/d456_camera/d456_camera.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
    </collision>
  </link>

  <link name="front_right_wheel_link">
    <inertial>
      <mass value="2.27" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0255" ixy="0.0" ixz="0.0" iyy="0.0255" iyz="0.0" izz="0.0510" />
    </inertial>
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/wheel/wheel.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0" />
      <material name="black" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/wheel/wheel.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0" />
    </collision>
  </link>

  <link name="front_left_wheel_link">
    <inertial>
      <mass value="2.27" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0255" ixy="0.0" ixz="0.0" iyy="0.0255" iyz="0.0" izz="0.0510" />
    </inertial>
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/wheel/wheel.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="-1.5708 0 0" />
      <material name="black" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/wheel/wheel.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="-1.5708 0 0" />
    </collision>
  </link>

  <link name="rear_right_wheel_link">
    <inertial>
      <mass value="2.27" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0255" ixy="0.0" ixz="0.0" iyy="0.0255" iyz="0.0" izz="0.0510" />
    </inertial>
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/wheel/wheel.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0" />
      <material name="black" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/wheel/wheel.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0" />
    </collision>
  </link>

  <link name="rear_left_wheel_link">
    <inertial>
      <mass value="2.27" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0255" ixy="0.0" ixz="0.0" iyy="0.0255" iyz="0.0" izz="0.0510" />
    </inertial>
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/wheel/wheel.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="-1.5708 0 0" />
      <material name="black" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/wheel/wheel.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="-1.5708 0 0" />
    </collision>
  </link>

  <joint name="base_rear_right_wheel_joint" type="continuous">
    <parent link="base_link" />
    <child link="rear_right_wheel_link" />
    <origin xyz="-0.19 -0.25 0.085" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <joint name="base_rear_left_wheel_joint" type="continuous">
    <parent link="base_link" />
    <child link="rear_left_wheel_link" />
    <origin xyz="-0.19 0.25 0.085" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <joint name="base_front_right_wheel_joint" type="continuous">
    <parent link="base_link" />
    <child link="front_right_wheel_link" />
    <origin xyz="0.28 -0.25 0.085" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <joint name="base_front_left_wheel_joint" type="continuous">
    <parent link="base_link" />
    <child link="front_left_wheel_link" />
    <origin xyz="0.28 0.25 0.085" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <joint name="base_lidar_joint" type="fixed">
    <parent link="base_link" />
    <child link="lidar_link" />
    <origin xyz="-0.0925 0 0.6075" rpy="0 0 0" />
  </joint>

  <joint name="base_d455_joint" type="fixed">
    <parent link="base_link" />
    <child link="d455_link" />
    <origin xyz="-0.305 0 0.45" rpy="0 0 3.14" />
  </joint>

  <joint name="base_d456_joint" type="fixed">
    <parent link="base_link" />
    <child link="d456_link" />
    <origin xyz="0.305 0 0.55" rpy="0 0 0" />
  </joint>

</robot>