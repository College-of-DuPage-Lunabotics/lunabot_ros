      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 1.5708" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/bulldozer_bot/bulldozer_bot_blade.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 1.5708" />
    </collision>
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0833" ixy="0.0" ixz="0.0" iyy="0.1667" iyz="0.0" izz="0.2333" />
    </inertial>
  </link>

  <link name="blade_arm_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/bulldozer_bot/bulldozer_bot_blade_arm.dae" />
      </geometry>
      <origin xyz="0.127 0 0" rpy="-1.5708 0 1.5708" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/bulldozer_bot/bulldozer_bot_blade_arm.stl" />
      </geometry>
      <origin xyz="0.127 0 0" rpy="-1.5708 0 1.5708" />
    </collision>
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0833" ixy="0.0" ixz="0.0" iyy="0.1667" iyz="0.0" izz="0.2333" />
    </inertial>
  </link>

  <link name="d455_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/d455_camera/d455_camera.stl" />
      </geometry>
      <origin xyz="0 0.015 0" rpy="0 0 0" />
      <material name="camera_grey" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/d455_camera/d455_camera.stl" />
      </geometry>
      <origin xyz="0 0.015 0" rpy="0 0 0" />
    </collision>
  </link>

  <link name="d456_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/d456_camera/d456_camera.stl" />
      </geometry>
      <origin xyz="0 -0.015 0" rpy="0 0 0" />
      <material name="camera_grey" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/d456_camera/d456_camera.stl" />
      </geometry>
      <origin xyz="0 -0.015 0" rpy="0 0 0" />
    </collision>
  </link>

  <link name="s3_lidar_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/s3_lidar/s3_lidar.stl" />
      </geometry>
      <origin xyz="0 -0.0155 0" rpy="0 0 -1.5708" />
      <material name="lidar_black" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/s3_lidar/s3_lidar.stl" />
      </geometry>
      <origin xyz="0 -0.0155 0" rpy="0 0 -1.5708" />
    </collision>
  </link>

  <link name="s2l_lidar_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/s2l_lidar/s2l_lidar.stl" />
      </geometry>
      <origin xyz="0 -0.015 0" rpy="0 3.14 -1.5708" />
      <material name="lidar_black" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/s2l_lidar/s2l_lidar.stl" />
      </geometry>
      <origin xyz="0 -0.015 0" rpy="0 0 -1.5708" />
