<?xml version="1.0"?>
<robot name="bulldozer_bot"
  xmlns:xacro="http://www.ros.org/wiki/xacro">

  <material name="camera_grey">
    <color rgba="0.81 0.81 0.81 1.0" />
  </material>

  <material name="lidar_black">
    <color rgba="0.22 0.22 0.22 1.0" />
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/bulldozer_bot/bulldozer_bot_drivetrain.dae" />
      </geometry>
      <origin xyz="0 0 0.1625" rpy="0 0 1.5708" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/models/bulldozer_bot/bulldozer_bot_drivetrain.stl" />
      </geometry>
      <origin xyz="0 0 0.1625" rpy="0 0 1.5708" />
    </collision>
    <inertial>
      <mass value="25.0" />
      <origin xyz="0 0 0.1625" />
      <inertia ixx="1.3669" ixy="0.0" ixz="0.0" iyy="3.3602" iyz="0.0" izz="4.7271" />
    </inertial>
  </link>

  <link name="blade_link">
